
  {
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "Referrer-Policy",
            "value" : "strict-origin"
          },
          {
            "key" : "Permissions-Policy",
            "value" : "geolocation=(self), microphone=()"
          }
        ]
      }
    ],
    "build": {
      "env": {
          "HUGO_VERSION": "0.119.0"
      }
  }
  
  }
